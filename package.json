{
  "name": "iron-language-support",
  "displayName": "iron language support",
  "description": "Language support for the iron programming language",
  "version": "0.1.0",
  "publisher": "emm312",
  "repository": "https://github.com/emm312/iron-syntax-highlighting/",
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "iron",
        "aliases": [
          "iron",
          "iron"
        ],
        "extensions": [
          ".iron"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "iron",
        "scopeName": "source.iron",
        "path": "./syntaxes/iron.tmLanguage.json"
      }
    ]
  }
}
